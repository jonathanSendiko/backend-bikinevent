{
  "kind": "collectionType",
  "collectionName": "produks",
  "info": {
    "name": "produk",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nama": {
      "type": "string"
    },
    "lokasi": {
      "type": "string"
    },
    "foto_produk": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "deskripsi_produk": {
      "type": "richtext"
    },
    "harga": {
      "type": "integer"
    },
    "vendor": {
      "via": "produks",
      "model": "vendor"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Perlengkapan",
        "Venue",
        "Talent",
        "Jasa",
        "Catering",
        "Dekorasi"
      ]
    },
    "rating": {
      "type": "decimal"
    },
    "penilaian": {
      "type": "component",
      "repeatable": true,
      "component": "vendor-related.penilaian"
    },
    "isArchived": {
      "type": "boolean"
    },
    "subcategory": {
      "type": "string"
    },
    "tags": {
      "collection": "tags"
    }
  }
}
