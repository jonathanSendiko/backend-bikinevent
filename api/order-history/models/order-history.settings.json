{
  "kind": "collectionType",
  "collectionName": "order_histories",
  "info": {
    "name": "order_history",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "produks": {
      "collection": "produk"
    },
    "pembeli": {
      "via": "order_histories",
      "model": "pembeli"
    },
    "rating": {
      "type": "float"
    },
    "informasi_pembeli": {
      "type": "component",
      "repeatable": false,
      "component": "user-related.informasi-pembeli"
    },
    "metode_pembayaran": {
      "type": "string"
    },
    "sistem_dp": {
      "type": "boolean",
      "default": false
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Paid",
        "Completed"
      ]
    },
    "total_price": {
      "type": "integer"
    },
    "notes": {
      "type": "component",
      "repeatable": true,
      "component": "user-related.notes"
    },
    "vendors": {
      "via": "order_histories",
      "collection": "vendor",
      "dominant": true
    },
    "kode_invoice": {
      "type": "string"
    },
    "link_invoice": {
      "type": "string"
    }
